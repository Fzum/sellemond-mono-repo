{
  "projectType": "library",
  "sourceRoot": "libs/portfolio/feature-projects/src",
  "prefix": "portfolio",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/portfolio/feature-projects"],
      "options": {
        "jestConfig": "libs/portfolio/feature-projects/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/portfolio/feature-projects/**/*.ts",
          "libs/portfolio/feature-projects/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:portfolio", "type:feature"]
}
